{
    "name": "ols-viikkopelit",
    "version": "1.0.0",
    "description": "",
    "main": "dist/app.js",
    "type": "module",
    "engines": {
        "node": ">=18.0.0"
    },
    "scripts": {
        "start": "node dist/app.js",
        "dev": "npm run build:css & node --import \"data:text/javascript,import { register } from 'node:module'; import { pathToFileURL } from 'node:url'; register('ts-node/esm', pathToFileURL('./'));\" src/app.ts",
        "start:server": "node --import \"data:text/javascript,import { register } from 'node:module'; import { pathToFileURL } from 'node:url'; register('ts-node/esm', pathToFileURL('./'));\" src/app.ts",
        "build": "tsc && npm run build:css:once",
        "build:css:once": "tailwindcss -i ./src/input.css -o ./public/css/style.css && ./node_modules/.bin/csso ./public/css/style.css -o ./public/css/style.min.css",
        "build:css": "tailwindcss -i ./src/input.css -o ./public/css/style.css --watch"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.9.0",
        "dotenv": "^16.5.0",
        "ejs": "^3.1.10",
        "express": "^5.1.0"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/ejs": "^3.1.5",
        "@types/express": "^5.0.1",
        "@types/node": "^20.12.12",
        "autoprefixer": "^10.4.21",
        "csso-cli": "^4.0.2",
        "postcss": "^8.5.3",
        "tailwindcss": "^3.4.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.5"
    }
}
